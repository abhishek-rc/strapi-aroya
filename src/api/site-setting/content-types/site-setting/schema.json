      "required": true,
      "default": "WEB",
      "enum": [
        "WEB",
        "MBA"
      ]
    },
    "pages": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::page.page",
      "mappedBy": "site_setting"
    }
  }
}
